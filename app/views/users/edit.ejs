<div class="page-header"><h1>Edit users</h1></div>

<% form_for(users, {action: path_to.users(users), method: 'PUT', id: "users_form", class: 'form-horizontal'}, function (form) { %>
    <%- partial('users/form.ejs', {locals: {form: form, users: users}}) %>
    <div class="form-actions">
        <%- form.submit('<i class="icon-ok icon-white"></i> Update users', {class: 'btn btn-primary'}) %> or
        <%- link_to('Cancel', path_to.users(users), {class: 'btn'}) %>
    </div>
<% });%>